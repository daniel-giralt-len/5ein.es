					"additionalProperties": false
				},
				{
					"type": "object",
					"properties": {
						"mode": {"const": "replaceSpells"},
						"spells": {"type": "object"},
						"daily": {"type": "object"}
					},
					"required": ["mode"],
					"additionalProperties": false
				},
				{
					"type": "object",
					"properties": {
						"mode": {"const": "addSpells"},
